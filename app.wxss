page {
  background-color: #f8f8f8;
}

button::after {
  content: none !important;
}

.container {
  height: 100%;
  color: #333;
}

.m_wrap {
  padding: 0 30rpx;
}

.fr {
  float: right !important;
}

.fl {
  float: left !important;
}

.txt_center {
  text-align: center;
}

.txt_left {
  text-align: left;
}

.txt_right {
  text-align: right;
}

.load_more_btn {
  height: 80rpx;
  line-height: 80rpx;
  background-color: #fff;
  margin: 30rpx;
  color: #333;
}

.load_more_btn[disabled] {
  background-color: rgba(0, 0, 0, 0.1) !important;
  color: #999 !important;
}

.nodata {
  clear: both;
  padding: 250rpx 0;
  margin: 0 auto;
  color: #333;
  text-align: center;
}

.navigator-box {
  height: 66rpx;
}

.navigator {
  top: 0;
  left: 0;
  z-index: 888;
  display: -webkit-box;
  display: -moz-box;
  display: -ms-flexbox;
  display: -webkit-flex;
  display: -moz-flex;
  display: -ms-flex;
  display: -o-flex;
  display: flex;
  -ms-align-items: center;
  align-items: center;
  -webkit-justify-content: space-between;
  -moz-justify-content: space-between;
  -ms-justify-content: space-between;
  -o-justify-content: space-between;
  justify-content: space-between;
  background-color: #05101c;
  border-bottom: 1px solid #121c28;
  color: #fff;
}

.navigator .n-item {
  -webkit-box-flex: 1;
  -moz-box-flex: 1;
  -o-box-flex: 1;
  flex-shrink: 0;
  height: 66rpx;
  line-height: 66rpx;
}

.navigator .n-left {
  margin-left: 30rpx;
}

.navigator .n-right {
  text-align: right;
  margin-right: 30rpx;
}

.navigator .n-center {
  flex-shrink: 1;
  width: auto;
  height: 66rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  text-align: center;
}

.city {
  font-size: 28rpx;
  line-height: 66rpx;
  margin-left: 20rpx;
  color: #333;
}

.icon_down {
  width: 30rpx;
  height: 20rpx;
  margin-top: 23rpx;
  margin-left: 14rpx;
  padding-right: 20rpx;
}

.m-section {
  background-color: #fff;
}

.v_checkbox {
  width: 40rpx;
  height: 40rpx;
  display: block;
  border: 1rpx solid #666;
  background-color: #fff;
  background-size: cover;
}

.v_checkbox.checked {
  background-image: url('data:image/png;base64,');
}

.v_radio {
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
  display: block;
  position: relative;
  border: 1rpx solid #62656a;
}

.v_radio.checked .v_radio_s {
  width: 24rpx;
  height: 24rpx;
  position: absolute;
  left: 50%;
  top: 50%;
  border-radius: 50%;
  margin-left: -12rpx;
  margin-top: -12rpx;
  display: block;
  background-color: #f33f40;
}

.fix_btns {
  color: white;
  position: fixed;
  bottom: 0;
  width: 100%;
  font-size: 30rpx;
  height: 98rpx;
  line-height: 98rpx;
  background-color: #fff;
  border-top: 1px solid #fff;
  z-index: 999;
}

.fix_btns .buy_btn {
  margin: 0;
  background-color: #f33f40;
  border: 1px solid #f33f40;
  color: #fff;
  display: inline;
  line-height: 98rpx;
  padding: 20rpx 50rpx;
  font-size: 34rpx;
  border-radius: 10rpx;
}

.fix_btns .m-wrap-rl {
  display: flex;
  padding: 0 25rpx;
}

.fix_btns .buy-count {
  flex: 1;
  color: #f33f40;
  font-size: 36rpx;
  text-align: center;
}

.fix_btns .buy_btn[disabled] {
  color: #d8d8d8 !important;
  background-color: #828282 !important;
}

.flex {
  display: flex;
}

.flex-item {
  flex: 1;
}

.txt-left {
  text-align: left !important;
}

.nodata {
  text-align: center;
}

.nodata .nodata_img {
  height: 128rpx;
  width: 128rpx;
}

.nodata .nodata_txt {
  text-align: center;
  font-size: 30rpx;
  color: #333;
  margin-top: 20rpx;
}

.loading_gif {
  width: 120rpx;
  height: 120rpx;
  position: fixed;
  z-index: 999;
  left: 50%;
  top: 50%;
  margin-left: -60rpx;
  margin-top: -60rpx;
}