  width:100%;
  display:flex;
  justify-content: center;
}
.listType{
  width:688rpx;
  height:60rpx;
  margin:20rpx;
  position: relative;
  border-radius: 30rpx;
  border:1px solid #e5d1a1;
}
.xihaList{
  width:380rpx;
  height:60rpx;
  border-radius: 30rpx;
  line-height:60rpx;
  font-size:30rpx;
  text-align: center;
  position: absolute;
  left:0;
  top:0;
  z-index:2;
}
.denonList{
  width:380rpx;
  height:60rpx;
  border-radius: 30rpx;
  line-height:60rpx;
  font-size:30rpx;
  text-align: center;
  position: absolute;
  right:0;
  top:0;
  z-index:3;
}
.listActive{
  color:#222;
